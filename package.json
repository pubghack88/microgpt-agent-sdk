{
  "name": "microgpt-agent-sdk",
  "version": "0.1.0",
  "description": "Production-ready TypeScript SDK for building AI agents with advanced memory, orchestration, and testing capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agents",
    "memory",
    "orchestration",
    "testing",
    "typescript",
    "openai",
    "anthropic",
    "langchain",
    "mcp",
    "multi-agent"
  ],
  "author": "Cognio AI Lab <dev@cogniolab.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cogniolab/microgpt-agent-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/cogniolab/microgpt-agent-sdk/issues"
  },
  "homepage": "https://github.com/cogniolab/microgpt-agent-sdk#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@supabase/supabase-js": "^2.39.0",
    "ioredis": "^5.3.2",
    "langchain": "^0.1.0",
    "openai": "^4.28.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@pinecone-database/pinecone": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@pinecone-database/pinecone": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
